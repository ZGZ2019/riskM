{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run risk_management",
      "runtimeExecutable": "cds",
      "killBehavior": "polite",
      "request": "launch",
      "args": [
        "watch",
        "--open"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "PreLaunchTask for Run risk_management",
      "envFile": "${workspaceFolder}/env/.env1",
      "env": {
        "run.config": "{\"handlerId\":\"lcap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/risk_management\\\",\\\"profileName\\\":\\\"\\\"}\"}"
      }
    },
    {
      "name": "apihub_sandbox",
      "runtimeExecutable": "cds",
      "request": "launch",
      "killBehavior": "polite",
      "args": [
        "watch",
        "--open"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/",
      "preLaunchTask": "dev-project",
      "envFile": "${workspaceFolder}/env/.env2",
      "env": {
        "run.config": "{\"handlerId\":\"lcap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/risk_management/\\\",\\\"profileName\\\":\\\"\\\"}\"}"
      }
    },
    {
      "name": "Run risk_management- with live data",
      "runtimeExecutable": "cds",
      "request": "launch",
      "killBehavior": "polite",
      "args": [
        "watch",
        "--open"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "PreLaunchTask for Run risk_management- with live data",
      "envFile": "${workspaceFolder}/env/.env3",
      "env": {
        "run.config": "{\"handlerId\":\"lcap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/risk_management\\\",\\\"profileName\\\":\\\"\\\"}\"}",
        "runConfigurations": "{\"odataBinding\":{\"apihub_sandbox\":{\"destination\":\"apihub_sandbox\",\"type\":\"Destination\",\"path\":\"/s4hanacloud/sap/opu/odata/sap/API_BUSINESS_PARTNER\"}}}"
      }
    }
  ]
}